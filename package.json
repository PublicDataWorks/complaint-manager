{
  "name": "complaint-manager",
  "version": "3.8.0",
  "private": true,
  "proxy": "https://localhost:1234",
  "scripts": {
    "postbuild": "node scripts/postbuild.js",
    "start": "yarn setup:db && concurrently 'yarn start:client' 'yarn start:server'",
    "start:dev": "yarn setup:db && concurrently 'yarn start:client' 'yarn start:server:dev'",
    "setup:db": "sequelize db:create && node scripts/loadSchema.js; sequelize db:migrate && sequelize db:seed:all;",
    "setup:test:db": "./scripts/setup-test-db.sh",
    "drop:db": "sequelize db:drop",
    "start:client": "HTTPS=true PORT=443 react-scripts start",
    "start:server:dev": "nodemon --watch src/server --watch src/sharedUtilities src/startServer.js --exec babel-node",
    "start:worker:dev": "nodemon --watch src/worker --watch src/sharedUtilities src/startWorker.js --exec babel-node",
    "start:server": "node dist/startServer.js",
    "start:worker": "node dist/startWorker.js",
    "e2e": "yarn e2e:cm && yarn e2e:mm",
    "e2e:cm": "./scripts/run-e2e-cm.sh",
    "e2e:mm": "./scripts/run-e2e-mm.sh",
    "build": "react-scripts --max_old_space_size=4096 build && babel src --out-dir dist --copy-files",
    "update:officers": "node -e \"require('./scripts/updateOfficerDataFromS3')\"",
    "test:server": "yarn setup:test:db test1 && yarn setup:test:db test2 && react-scripts --max_old_space_size=1500 test src/server --logHeapUsage --maxWorkers=2 --forceExit --env=node",
    "test:server:ci": "CI=true yarn test:server",
    "test:server-handlers:ci": "yarn setup:test:db test1 && yarn setup:test:db test2 && CI=true react-scripts --max_old_space_size=1500 test src/server/handlers.* --logHeapUsage --maxWorkers=2 --forceExit --env=node",
    "test:server-worker-non-handlers:ci": "yarn setup:test:db test1 && yarn setup:test:db test2 && CI=true react-scripts test \"(?!.*handlers.*)src/(server|worker).*\" --logHeapUsage --maxWorkers=2 --forceExit --env=node",
    "test:worker": "yarn setup:test:db test1 && yarn setup:test:db test2 && react-scripts --max_old_space_size=1500 test src/worker --logHeapUsage --maxWorkers=2 --env=node",
    "test:worker:ci": "CI=true yarn test:worker",
    "test:client": "REACT_APP_ENV=test react-scripts test src/client --logHeapUsage --env=jsdom",
    "test:client:ci": "REACT_APP_ENV=test CI=true react-scripts test src/client --logHeapUsage --maxWorkers=2 --env=jsdom",
    "test:server:worker:ci": "NODE_ENV=test yarn test:server:ci && yarn test:worker:ci",
    "test:once": "run-p -c test:server:worker:ci test:client:ci",
    "test:debug": "NODE_ENV=test yarn setup:test:db && CI=true && react-scripts --max_old_space_size=1024 --inspect-brk=0.0.0.0 test src/server --logHeapUsage --runInBand --env=node",
    "eject": "react-scripts eject",
    "transformOfficerCsvData": "node -e \"require('./scripts/transformOfficerCsvData')\"",
    "normalizeSexAndRaceValues": "node -e \"require('./scripts/normalizeSexAndRaceValuesFromTransformedFile.js')\""
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "@hapi/hoek": "~9.0.3",
    "@material-ui/core": "^4.9.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.40",
    "@material-ui/styles": "^4.9.0",
    "@testing-library/dom": "6.16.0",
    "@testing-library/jest-dom": "~5.3.0",
    "@testing-library/react": "^9.4.0",
    "auth0": "~2.24.0",
    "auth0-js": "~9.12.2",
    "autosuggest-highlight": "~3.1.1",
    "aws-sdk": "~2.624.0",
    "axios": "~0.19.0",
    "body-parser": "~1.19.0",
    "boom": "~7.3.0",
    "bull": "^3.13.0",
    "bull-repl": "^0.23.0",
    "busboy": "~0.3.0",
    "connected-react-router": "~6.8.0",
    "cookie-parser": "~1.4.4",
    "csv-parse": "~4.8.8",
    "csv-stringify": "~5.3.0",
    "eventsourcemock": "^2.0.0",
    "express": "~4.17.1",
    "express-jwt": "~5.3.1",
    "express-jwt-authz": "~2.3.0",
    "express-winston": "~4.0.3",
    "fflip": "~4.0.0",
    "fflip-express": "~1.0.2",
    "file-saver": "~2.0.1",
    "handlebars": "^4.5.2",
    "helmet": "^3.21.1",
    "history": "~4.10.1",
    "html-pdf": "~2.2.0",
    "jsonwebtoken": "~8.5.0",
    "jwks-rsa": "1.6.0",
    "lodash": "~4.17.11",
    "moment": "~2.24.0",
    "moment-timezone": "~0.5.23",
    "newrelic": "~6.6.0",
    "node-cache": "^5.1.0",
    "nodemon": "~1.19.1",
    "pg": "7.12.1",
    "pg-hstore": "~2.3.2",
    "plotly.js": "^1.52.2",
    "rc-pagination": "^2.2.2",
    "react": "~16.12.0",
    "react-autosuggest": "~9.4.3",
    "react-dom": "~16.12.0",
    "react-dropzone-component": "~3.2.0",
    "react-pdf": "~4.1.0",
    "react-plotly.js": "^2.4.0",
    "react-quill": "~1.3.3",
    "react-redux": "^7.2.0",
    "react-router": "~5.0.0",
    "react-router-dom": "~5.0.0",
    "react-scripts": "~3.0.1",
    "react-text-mask": "~5.4.3",
    "redux": "~4.0.1",
    "redux-form": "~8.2.5",
    "redux-form-material-ui": "^5.0.0-beta.3",
    "redux-thunk": "~2.3.0",
    "selenium-server": "~3.141.59",
    "sequelize": "~5.21.6",
    "sequelize-cli": "~5.5.0",
    "shortid": "~2.2.14",
    "striptags": "~3.1.1",
    "timekeeper": "~2.2.0",
    "validator": "~13.0.0",
    "winston": "3.3.1"
  },
  "devDependencies": {
    "@babel/cli": "~7.8.4",
    "@babel/core": "~7.9.0",
    "@babel/node": "~7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-syntax-json-strings": "~7.8.3",
    "@babel/preset-env": "~7.9.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "~7.9.0",
    "@testing-library/user-event": "^10.0.1",
    "child-process-promise": "^2.2.1",
    "chromedriver": "^83",
    "concurrently": "~5.1.0",
    "enzyme": "~3.10.0",
    "enzyme-adapter-react-16": "~1.14.0",
    "enzyme-to-json": "~3.4.0",
    "jest-extended": "^0.11.1",
    "nightwatch": "~1.3.2",
    "nock": "10.0.6",
    "node-mocks-http": "~1.7.3",
    "prettier": "~2.0.2",
    "promise-retry": "2.0.1",
    "redux-devtools-extension": "~2.13.8",
    "supertest": "~4.0.0",
    "yarn-run-all": "~3.1.1"
  },
  "resolutions": {
    "**/set-value": "^2.0.1",
    "**/mixin-deep": "^1.3.2",
    "**/extend": "3.0.2",
    "**/eslint-utils": "2.0.0",
    "**/serialize-javascript": "^2.1.1",
    "**/tree-kill": "^1.2.2",
    "**/mem": "^4.0.0",
    "**/static-eval": "^2.0.5",
    "**/acorn": "^6.4.1",
    "**/minimist": "^1.2.5",
    "**/yargs-parser": "^13.1.2",
    "**/http-proxy": "^1.18.1"
  }
}
