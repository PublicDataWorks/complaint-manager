{
  "name": "complaint-manager",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:1234",
  "scripts": {
    "start": "yarn setup:db && concurrently 'yarn start:client' 'yarn start:server'",
    "setup:db": "sequelize db:create; sequelize db:migrate && sequelize db:seed:all",
    "drop:db": "sequelize db:drop",
    "setup:db:prod": "sequelize db:create; sequelize db:migrate",
    "start:client": "react-scripts start",
    "start:server": "nodemon --watch src/server src/startServer.js",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:once": "CI=true yarn test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "auth0-js": "^9.1.3",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "express-jwt": "^5.3.0",
    "history": "^4.7.2",
    "jwks-rsa": "^1.2.1",
    "lodash": "^4.17.4",
    "material-ui": "^1.0.0-beta.30",
    "material-ui-icons": "^1.0.0-beta.17",
    "material-ui-pickers": "^1.0.0-beta.13.1",
    "moment": "^2.20.1",
    "nightwatch": "^0.9.19",
    "nodemailer": "^4.4.1",
    "nodemon": "^1.14.9",
    "password-generator": "^2.2.0",
    "pg": "6.4.2",
    "pg-hstore": "^2.3.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "1.0.17",
    "redux": "^3.7.2",
    "redux-form": "^7.2.1",
    "redux-form-material-ui": "^5.0.0-beta.2",
    "redux-thunk": "^2.2.0",
    "sequelize": "^4.28.2",
    "sequelize-cli": "^3.2.0",
    "validator": "^9.2.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "nock": "^9.1.5",
    "node-mocks-http": "^1.6.6",
    "promise-retry": "^1.1.1",
    "redux-devtools-extension": "^2.13.2",
    "smtp-tester": "1.1.0",
    "supertest": "^3.0.0"
  }
}
