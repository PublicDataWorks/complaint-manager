{
  "name": "police-data-manager",
  "version": "3.33.1",
  "private": true,
  "proxy": "https://localhost:1234",
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "postbuild": "node scripts/postbuild.js",
    "start": "yarn setup:db && concurrently 'yarn start:client' 'yarn start:server'",
    "setup:localstack": "./scripts/init-localstack.sh",
    "start:dev": "yarn setup:localstack && yarn setup:db && concurrently 'yarn start:client' 'yarn start:server:dev'",
    "setup:db": "sequelize db:create; node scripts/checkEmptySchema.js && node scripts/loadSchema.js; sequelize db:migrate && sequelize db:seed:all",
    "setup:test:db": "./scripts/setup-test-db.sh",
    "drop:db": "sequelize db:drop",
    "start:client": "GENERATE_SOURCEMAP=false HTTPS=true PORT=443 react-scripts start",
    "start:server:dev": "nodemon --watch src/server --watch src/sharedUtilities src/startServer.js --exec babel-node",
    "start:worker:dev": "nodemon --watch src/worker --watch src/sharedUtilities src/startWorker.js --exec babel-node",
    "start:server": "node dist/startServer.js",
    "start:worker": "node dist/startWorker.js",
    "e2e": "yarn e2e:pdm",
    "e2e:pdm": "./scripts/run-e2e-pdm.sh",
    "build": "INLINE_RUNTIME_CHUNK=false react-scripts --max_old_space_size=4096 build && babel src --out-dir dist --copy-files",
    "update:officers": "node -e \"require('./scripts/updateOfficerDataFromS3')\"",
    "test:server": "yarn setup:test:db test1 && yarn setup:test:db test2 && react-app-rewired --max_old_space_size=1500 test src/server --logHeapUsage --maxWorkers=2 --forceExit --env=node",
    "test:server:ci": "CI=true yarn test:server --coverage  --collectCoverageFrom='src/server/**/*.{js,jsx}'",
    "test:server-handlers:ci": "yarn setup:test:db test1 && yarn setup:test:db test2 && CI=true react-app-rewired --max_old_space_size=1500 test src/server/handlers.* --logHeapUsage --maxWorkers=2 --forceExit --env=node  --coverage  --collectCoverageFrom='src/server/handlers/**/*.{js,jsx}'",
    "test:server-worker-non-handlers:ci": "yarn setup:test:db test1 && yarn setup:test:db test2 && CI=true react-app-rewired test \"(?!.*handlers.*)src/(server|worker).*\" --logHeapUsage --maxWorkers=2 --forceExit --env=node  --coverage --collectCoverageFrom='src/{server,worker}/!(handlers|migrations|seeders)/**/*.{js,jsx}'",
    "test:worker": "yarn setup:test:db test1 && yarn setup:test:db test2 && react-app-rewired --max_old_space_size=1500 test src/worker --logHeapUsage --maxWorkers=2 --env=node",
    "test:worker:ci": "CI=true yarn test:worker",
    "test:client": "REACT_APP_ENV=test react-app-rewired test src/client --logHeapUsage --env=jsdom",
    "test:client:ci": "REACT_APP_ENV=test CI=true react-app-rewired test src/client --logHeapUsage --maxWorkers=2 --env=jsdom --coverage --collectCoverageFrom='src/client/**/*.{js,jsx}'",
    "test:server:worker:ci": "NODE_ENV=test yarn test:server:ci && yarn test:worker:ci",
    "test:once": "run-p -c test:server:worker:ci test:client:ci",
    "test:debug": "NODE_ENV=test yarn setup:test:db && CI=true && react-app-rewired --max_old_space_size=1024 --inspect-brk=0.0.0.0 test src/server --logHeapUsage --runInBand --env=node",
    "eject": "react-scripts eject",
    "transformOfficerCsvData": "node -e \"require('./scripts/transformOfficerCsvData')\"",
    "normalizeSexAndRaceValues": "node -e \"require('./scripts/normalizeSexAndRaceValuesFromTransformedFile.js')\""
  },
  "jest": {
    "snapshotResolver": "./snapshotResolver.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coverageReporters": [
      "text",
      "text-summary"
    ],
    "resetMocks": false
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.10.0",
    "@hapi/hoek": "~9.0.3",
    "@material-ui/core": "~4.10.2",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.40",
    "@material-ui/styles": "~4.10.0",
    "@testing-library/dom": "~7.16.2",
    "@testing-library/jest-dom": "~5.10.0",
    "@testing-library/react": "~11.0.0",
    "auth0": "2.31.1",
    "auth0-js": "~9.12.2",
    "autosuggest-highlight": "~3.1.1",
    "aws-sdk": "^2.722.0",
    "axios": "0.21.1",
    "babel-jest": "26.6.3",
    "body-parser": "~1.19.0",
    "boom": "~7.3.0",
    "bull": "^3.13.0",
    "bull-repl": "^0.23.0",
    "busboy": "~0.3.0",
    "compression": "^1.7.4",
    "connected-react-router": "~6.8.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "css-mediaquery": "^0.1.2",
    "csv-parse": "~4.8.8",
    "csv-stringify": "~5.3.0",
