{
  "name": "complaint-manager",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:1234",
  "scripts": {
    "start": "yarn setup:db && concurrently 'yarn start:client' 'yarn start:server'",
    "setup:db": "sequelize db:create; sequelize db:migrate && sequelize db:seed:all",
    "setup:db:prod": "sequelize db:create; sequelize db:migrate",
    "start:client": "react-scripts start",
    "start:server": "node src/startServer.js --color",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:once": "CI=true yarn test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "express": "^4.16.2",
    "lodash": "^4.17.4",
    "material-ui": "^1.0.0-beta.24",
    "material-ui-icons": "^1.0.0-beta.17",
    "pg": "6.4.2",
    "pg-hstore": "^2.3.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "redux": "^3.7.2",
    "redux-form": "^7.2.0",
    "redux-thunk": "^2.2.0",
    "sequelize": "^4.28.2",
    "sequelize-cli": "^3.2.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "nock": "^9.1.5",
    "redux-devtools-extension": "^2.13.2"
  }
}
