    "express": "~4.17.1",
    "express-jwt": "^6.0.0",
    "express-jwt-authz": "~2.3.0",
    "express-sslify": "^1.2.0",
    "express-winston": "~4.0.3",
    "fflip": "~4.0.0",
    "fflip-express": "~1.0.2",
    "file-saver": "~2.0.1",
    "handlebars": "^4.5.2",
    "helmet": "^4.1.0",
    "history": "~4.10.1",
    "html-pdf": "~2.2.0",
    "jsonwebtoken": "~8.5.0",
    "jwks-rsa": "1.6.0",
    "lodash": "4.17.20",
    "moment": "~2.24.0",
    "moment-timezone": "~0.5.23",
    "newrelic": "^7.0.2",
    "node-cache": "^5.1.0",
    "pg": "8.0.3",
    "pg-hstore": "~2.3.2",
    "plotly.js": "^1.58.4",
    "rc-pagination": "^3.1.2",
    "react": "~16.12.0",
    "react-app-rewired": "^2.1.8",
    "react-autosuggest": "~9.4.3",
    "react-dom": "~16.12.0",
    "react-dropzone-component": "~3.2.0",
    "react-ga": "^3.3.0",
    "react-pdf": "~4.1.0",
    "react-plotly.js": "^2.4.0",
    "react-quill": "~1.3.3",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.0",
    "react-router-dom": "^5.1.0",
    "react-scripts": "4.0.1",
    "react-text-mask": "~5.4.3",
    "redux": "~4.0.1",
    "redux-form": "~8.2.5",
    "redux-form-material-ui": "^5.0.0-beta.3",
    "redux-thunk": "~2.3.0",
    "sequelize": "~5.21.6",
    "sequelize-cli": "~5.5.0",
    "shortid": "~2.2.14",
    "striptags": "~3.1.1",
    "timekeeper": "~2.2.0",
    "validator": "13.1.1",
    "winston": "3.3.1",
    "winston-transport": "^4.4.0"
  },
  "devDependencies": {
    "@babel/cli": "~7.10.3",
    "@babel/core": "~7.10.3",
    "@babel/node": "~7.10.3",
    "@babel/plugin-proposal-class-properties": "~7.10.1",
    "@babel/plugin-syntax-json-strings": "~7.8.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "~7.10.1",
    "@babel/register": "~7.10.3",
    "@sheerun/mutationobserver-shim": "^0.3.3",
    "@testing-library/user-event": "~12.0.6",
    "child-process-promise": "^2.2.1",
    "chromedriver": "^87.0.7",
    "concurrently": "~5.1.0",
    "enzyme": "~3.10.0",
    "enzyme-adapter-react-16": "~1.14.0",
