{
  "name": "complaint-manager",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:1234",
  "scripts": {
    "start": "yarn setup:db && concurrently 'yarn start:client' 'yarn start:server'",
    "setup:db": "sequelize db:create; sequelize db:migrate; sequelize db:seed:all",
    "setup:test:db": "sequelize db:create; sequelize db:migrate",
    "drop:db": "sequelize db:drop",
    "start:client": "react-scripts start",
    "start:server": "nodemon --watch src/server src/startServer.js",
    "e2e": "nightwatch -c e2e/nightwatch.local.json",
    "build": "react-scripts build",
    "test:all": "NODE_ENV=test yarn setup:test:db && react-scripts test --maxWorkers=1",
    "test:server": "NODE_ENV=test yarn setup:test:db && react-scripts test src/server --maxWorkers=1",
    "test:server:ci": "CI=true react-scripts test src/server --maxWorkers=1 --forceExit",
    "test:client": "react-scripts test src/client --env=jsdom",
    "test:client:ci": "CI=true react-scripts test src/client --env=jsdom",
    "test:once": "sleep 10 && NODE_ENV=test yarn setup:test:db && yarn test:client:ci && yarn test:server:ci",
    "eject": "react-scripts eject"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "@material-ui/icons": "^1.0.0-beta.43",
    "auth0": "^2.9.1",
    "auth0-js": "^9.1.3",
    "autosuggest-highlight": "^3.1.1",
    "aws-sdk": "^2.203.0",
    "body-parser": "^1.18.2",
    "boom": "^7.2.0",
    "busboy": "^0.2.14",
    "csv-parse": "^2.2.0",
    "csv-stringify": "^3.0.0",
    "express": "^4.16.2",
    "express-jwt": "^5.3.0",
    "express-jwt-authz": "^1.0.0",
    "express-winston": "^2.5.1",
    "file-saver": "^1.3.3",
    "helmet": "^3.12.0",
    "history": "^4.7.2",
    "jsonwebtoken": "^8.2.0",
    "jwks-rsa": "^1.2.1",
    "lodash": "^4.17.4",
    "material-ui": "1.0.0-beta.44",
    "moment": "^2.20.1",
    "moment-timezone": "^0.5.14",
    "nodemailer": "^4.4.1",
    "nodemon": "^1.14.9",
    "password-generator": "^2.2.0",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "react": "^16.2.0",
    "react-autosuggest": "^9.3.4",
    "react-dom": "^16.2.0",
    "react-dropzone-component": "^3.1.2",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "^1.1.1",
    "redux": "^4.0.0",
    "redux-form": "^7.2.1",
    "redux-form-material-ui": "^5.0.0-beta.2",
    "redux-thunk": "^2.2.0",
    "sequelize": "^4.37.6",
    "sequelize-cli": "^4.0.0",
    "validator": "^10.0.0",
    "winston": "2.4.2"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-to-json": "^3.3.3",
    "nightwatch": "^0.9.20",
    "nock": "^9.1.5",
    "node-mocks-http": "^1.6.6",
    "prettier": "^1.12.1",
    "promise-retry": "^1.1.1",
    "redux-devtools-extension": "^2.13.2",
    "smtp-tester": "^1.2.0",
    "supertest": "^3.0.0"
  }
}
