{
  "name": "complaint-manager",
  "version": "2.29.0",
  "private": true,
  "proxy": "http://localhost:1234",
  "scripts": {
    "postbuild": "node scripts/postbuild.js",
    "start": "yarn setup:db && yarn setup:task:migrate && concurrently 'yarn start:client' 'yarn start:server'",
    "start:dev": "yarn setup:db && yarn setup:task:migrate && concurrently 'yarn start:client' 'yarn start:server:dev'",
    "setup:db": "sequelize db:create; sequelize db:migrate && sequelize db:seed:all;",
    "setup:task:migrate": "node ./src/server/tasks/migrate.js up",
    "setup:test:db": "./scripts/setup-test-db.sh",
    "drop:db": "sequelize db:drop",
    "start:client": "react-scripts start",
    "start:server:dev": "nodemon --watch src/server --watch src/sharedUtilities src/startServer.js --exec babel-node",
    "start:worker:dev": "nodemon --watch src/worker --watch src/sharedUtilities src/startWorker.js --exec babel-node",
    "start:server": "node dist/startServer.js",
    "start:worker": "node dist/startWorker.js",
    "e2e": "nightwatch -c e2e/nightwatch.conf.local.js",
    "e2e:cm": "nightwatch -c e2e/nightwatch.conf.complaints.js",
    "e2e:dp": "nightwatch -c e2e/nightwatch.conf.disciplinary.js",
    "build": "react-scripts --max_old_space_size=3072 build && babel src --out-dir dist --copy-files",
    "update:officers": "node -e \"require('./scripts/updateOfficerDataFromS3')\"",
    "test:server": "yarn setup:test:db test1 && yarn setup:test:db test2 && react-scripts --max_old_space_size=1500 test src/server --logHeapUsage --maxWorkers=2 --forceExit --env=node",
    "test:server:ci": "CI=true yarn test:server",
    "test:server-handlers:ci": "yarn setup:test:db test1 && yarn setup:test:db test2 && CI=true react-scripts --max_old_space_size=1500 test src/server/handlers.* --logHeapUsage --maxWorkers=2 --forceExit --env=node",
    "test:server-worker-non-handlers:ci": "yarn setup:test:db test1 && yarn setup:test:db test2 && CI=true react-scripts test \"(?!.*handlers.*)src/(server|worker).*\" --logHeapUsage --maxWorkers=2 --forceExit --env=node",
    "test:worker": "yarn setup:test:db test1 && yarn setup:test:db test2 && react-scripts --max_old_space_size=1500 test src/worker --logHeapUsage --maxWorkers=2 --env=node",
    "test:worker:ci": "CI=true yarn test:worker",
    "test:client": "REACT_APP_ENV=test react-scripts test src/client --logHeapUsage --env=jsdom",
    "test:client:ci": "REACT_APP_ENV=test CI=true react-scripts test src/client --logHeapUsage --maxWorkers=2 --env=jsdom",
    "test:server:worker:ci": "NODE_ENV=test yarn test:server:ci && yarn test:worker:ci",
    "test:once": "run-p -c test:server:worker:ci test:client:ci",
    "test:debug": "NODE_ENV=test yarn setup:test:db && CI=true && react-scripts --max_old_space_size=1024 --inspect-brk=0.0.0.0 test src/server --logHeapUsage --runInBand --env=node",
    "eject": "react-scripts eject",
    "transformOfficerCsvData": "node -e \"require('./scripts/transformOfficerCsvData')\"",
    "normalizeSexAndRaceValues": "node -e \"require('./scripts/normalizeSexAndRaceValuesFromTransformedFile.js')\""
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "@material-ui/core": "~3.9.2",
    "@material-ui/icons": "~3.0.0",
    "auth0": "~2.18.0",
    "auth0-js": "~9.11.3",
    "autosuggest-highlight": "~3.1.1",
    "aws-sdk": "~2.504.0",
    "axios": "~0.19.0",
    "body-parser": "~1.19.0",
    "boom": "~7.3.0",
    "busboy": "~0.3.0",
    "chromedriver": "~76",
    "connected-react-router": "~6.5.2",
    "cookie-parser": "~1.4.4",
    "csv-parse": "~4.4.6",
    "csv-stringify": "~5.3.0",
    "express": "~4.17.1",
    "express-jwt": "~5.3.1",
    "express-jwt-authz": "~2.3.0",
    "express-winston": "~3.3.0",
    "fflip": "~4.0.0",
    "fflip-express": "~1.0.2",
    "file-saver": "~2.0.1",
    "handlebars": "~4.1.2",
    "helmet": "~3.20.0",
    "history": "~4.9.0",
    "html-pdf": "~2.2.0",
    "jsonwebtoken": "~8.5.0",
    "jwks-rsa": "1.6.0",
    "kue": "~0.11.6",
    "lodash": "~4.17.11",
    "moment": "~2.24.0",
    "moment-timezone": "~0.5.23",
    "newrelic": "~5.11.0",
    "nodemon": "~1.19.1",
    "pg": "~7.12.0",
    "pg-hstore": "~2.3.2",
    "rc-pagination": "~1.20.5",
    "react": "~16.8.4",
    "react-autosuggest": "~9.4.3",
    "react-dom": "~16.8.4",
    "react-dropzone-component": "~3.2.0",
    "react-pdf": "~4.1.0",
    "react-quill": "~1.3.3",
    "react-redux": "~6.0.1",
    "react-router": "~5.0.0",
    "react-router-dom": "~5.0.0",
    "react-scripts": "3.0.1",
    "react-select": "~3.0.4",
    "react-text-mask": "~5.4.3",
    "redux": "~4.0.1",
    "redux-form": "~8.2.5",
    "redux-form-material-ui": "~5.0.0-beta.3",
    "redux-thunk": "~2.3.0",
    "selenium-server": "~3.141.59",
    "sequelize": "~5.15.1",
    "sequelize-cli": "~5.5.0",
    "shortid": "~2.2.14",
    "striptags": "~3.1.1",
    "timekeeper": "~2.2.0",
    "umzug": "~2.2.0",
    "validator": "~10.11.0",
    "winston": "~3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "~7.5.5",
    "@babel/core": "~7.5.5",
    "@babel/node": "~7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-syntax-json-strings": "~7.2.0",
    "@babel/preset-env": "~7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "~7.5.5",
    "concurrently": "~4.1.0",
    "enzyme": "~3.10.0",
    "enzyme-adapter-react-16": "~1.14.0",
    "enzyme-to-json": "~3.4.0",
    "jest-extended": "^0.11.1",
    "nightwatch": "~1.1.13",
    "nock": "~10.0.6",
    "node-mocks-http": "~1.7.3",
    "prettier": "~1.18.2",
    "promise-retry": "~1.1.1",
    "redux-devtools-extension": "~2.13.8",
    "supertest": "~4.0.0",
    "yarn-run-all": "~3.1.1"
  },
  "resolutions": {
    "**/set-value": "^2.0.1",
    "**/mixin-deep": "^1.3.2",
    "**/extend": "3.0.2",
    "**/eslint-utils": "1.4.1"
  }
}
