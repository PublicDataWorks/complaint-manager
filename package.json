{
  "name": "complaint-manager",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:1234",
  "scripts": {
    "postbuild": "node scripts/postbuild.js",
    "start": "yarn setup:db && yarn setup:task:migrate && concurrently 'yarn start:client' 'yarn start:server'",
    "setup:db": "sequelize db:create; sequelize db:migrate; sequelize db:seed:all;",
    "setup:task:migrate": "node ./src/server/tasks/migrate.js up",
    "setup:test:db": "sequelize db:create; sequelize db:migrate",
    "drop:db": "sequelize db:drop",
    "start:client": "react-scripts start",
    "start:server": "nodemon --watch src/server src/startServer.js --exec babel-node",
    "start:worker": "nodemon --watch src/worker src/startWorker.js --exec babel-node",
    "e2e": "nightwatch -c e2e/nightwatch.local.json",
    "build": "react-scripts build",
    "update:officers": "node -e \"require('./scripts/updateOfficerDataFromS3')\"",
    "test:all": "NODE_ENV=test yarn setup:test:db && react-scripts test --logHeapUsage --maxWorkers=1 --env=node",
    "test:server": "NODE_ENV=test yarn setup:test:db && react-scripts test src/server --logHeapUsage --maxWorkers=1 --env=node",
    "test:server:ci": "NODE_ENV=test yarn setup:test:db && CI=true react-scripts test src/server --logHeapUsage --maxWorkers=1 --forceExit --env=node",
    "test:server-handlers:ci": "NODE_ENV=test yarn setup:test:db && CI=true react-scripts test src/server/handlers.* --logHeapUsage --maxWorkers=1 --forceExit --env=node",
    "test:server-worker-non-handlers:ci": "NODE_ENV=test yarn setup:test:db && CI=true react-scripts test \"(?!.*handlers.*)src/(server|worker).*\" --logHeapUsage --maxWorkers=1 --forceExit --env=node",
    "test:worker": "NODE_ENV=test yarn setup:test:db && react-scripts test src/worker --logHeapUsage --maxWorkers=1 --env=node",
    "test:worker:ci": "NODE_ENV=test yarn setup:test:db && CI=true react-scripts test src/worker --logHeapUsage --maxWorkers=1 --forceExit --env=node",
    "test:client": "react-scripts test src/client --logHeapUsage --env=jsdom",
    "test:client:ci": "CI=true react-scripts test src/client --logHeapUsage --maxWorkers=1 --env=jsdom",
    "test:server:worker:ci": "NODE_ENV=test yarn test:server:ci && yarn test:worker:ci",
    "test:once": "run-p -c test:server:worker:ci test:client:ci",
    "eject": "react-scripts eject",
    "transformOfficerCsvData": "node -e \"require('./scripts/transformOfficerCsvData')\""
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^3.0.0",
    "@material-ui/icons": "^3.0.0",
    "auth0": "^2.9.1",
    "auth0-js": "^9.1.3",
    "autosuggest-highlight": "^3.1.1",
    "aws-sdk": "^2.203.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "boom": "^7.2.0",
    "busboy": "^0.2.14",
    "cookie-parser": "^1.4.3",
    "csv-parse": "3.0.0",
    "csv-stringify": "^4.0.1",
    "express": "^4.16.2",
    "express-jwt": "^5.3.0",
    "express-jwt-authz": "^1.0.0",
    "express-winston": "^3.0.0",
    "fflip": "^4.0.0",
    "fflip-express": "^1.0.2",
    "file-saver": "^2.0.0-rc.2",
    "handlebars": "^4.0.12",
    "helmet": "^3.12.0",
    "history": "^4.7.2",
    "html-pdf": "^2.2.0",
    "jsonwebtoken": "^8.2.0",
    "jwks-rsa": "^1.2.1",
    "kue": "^0.11.6",
    "lodash": "^4.17.4",
    "moment": "^2.20.1",
    "moment-timezone": "^0.5.14",
    "newrelic": "^4.2.0",
    "nodemon": "^1.14.9",
    "pg": "^7.4.1",
    "pg-hstore": "^2.3.2",
    "rc-pagination": "^1.16.5",
    "react": "^16.4.0",
    "react-autosuggest": "^9.3.4",
    "react-dom": "^16.4.0",
    "react-dropzone-component": "^3.1.2",
    "react-pdf": "4.0.0-beta.6",
    "react-quill": "^1.3.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "^2.1.0",
    "react-text-mask": "^5.4.3",
    "redux": "^4.0.0",
    "redux-form": "^7.4.0",
    "redux-form-material-ui": "^5.0.0-beta.3",
    "redux-thunk": "^2.2.0",
    "sequelize": "^4.39.0",
    "sequelize-cli": "^5.1.0",
    "shortid": "^2.2.13",
    "striptags": "^3.1.1",
    "timekeeper": "^2.1.2",
    "umzug": "^2.1.0",
    "validator": "^10.0.0",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-syntax-json-strings": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "concurrently": "^4.0.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-to-json": "^3.3.3",
    "jest": "^23.6.0",
    "jest-each": "^23.5.0",
    "nightwatch": "^1.0.0",
    "nock": "^10.0.0",
    "node-mocks-http": "^1.6.6",
    "prettier": "^1.15.2",
    "promise-retry": "^1.1.1",
    "redux-devtools-extension": "^2.13.2",
    "smtp-tester": "^1.2.0",
    "supertest": "^3.0.0",
    "yarn-run-all": "^3.1.1"
  }
}
