{
  "name": "complaint-manager",
  "version": "2.17.0",
  "private": true,
  "proxy": "http://localhost:1234",
  "scripts": {
    "postbuild": "node scripts/postbuild.js",
    "start": "yarn setup:db && yarn setup:task:migrate && concurrently 'yarn start:client' 'yarn start:server'",
    "start:dev": "yarn setup:db && yarn setup:task:migrate && concurrently 'yarn start:client' 'yarn start:server:dev'",
    "setup:db": "sequelize db:create; sequelize db:migrate && sequelize db:seed:all;",
    "setup:task:migrate": "node ./src/server/tasks/migrate.js up",
    "setup:test:db": "sequelize db:create; sequelize db:migrate",
    "drop:db": "sequelize db:drop",
    "start:client": "react-scripts start",
    "start:server:dev": "nodemon --watch src/server --watch src/sharedUtilities src/startServer.js --exec babel-node",
    "start:worker:dev": "nodemon --watch src/worker --watch src/sharedUtilities src/startWorker.js --exec babel-node",
    "start:server": "node dist/startServer.js",
    "start:worker": "node dist/startWorker.js",
    "e2e": "nightwatch -c e2e/nightwatch.conf.local.js",
    "build": "react-scripts --max_old_space_size=2048 build && babel src --out-dir dist --copy-files",
    "update:officers": "node -e \"require('./scripts/updateOfficerDataFromS3')\"",
    "test:all": "NODE_ENV=test yarn setup:test:db && react-scripts test --logHeapUsage --maxWorkers=1 --env=node",
    "test:server": "NODE_ENV=test yarn setup:test:db && react-scripts test src/server --logHeapUsage --maxWorkers=1 --env=node",
    "test:server:ci": "NODE_ENV=test yarn setup:test:db && CI=true react-scripts --max_old_space_size=2048 test src/server --logHeapUsage --maxWorkers=1 --forceExit --env=node",
    "test:server-handlers:ci": "NODE_ENV=test yarn setup:test:db && CI=true react-scripts --max_old_space_size=2048 test src/server/handlers.* --logHeapUsage --maxWorkers=1 --forceExit --env=node",
    "test:server-worker-non-handlers:ci": "NODE_ENV=test yarn setup:test:db && CI=true react-scripts test \"(?!.*handlers.*)src/(server|worker).*\" --logHeapUsage --maxWorkers=1 --forceExit --env=node",
    "test:worker": "NODE_ENV=test yarn setup:test:db && react-scripts test src/worker --logHeapUsage --maxWorkers=1 --env=node",
    "test:worker:ci": "NODE_ENV=test yarn setup:test:db && CI=true react-scripts --max_old_space_size=2048 test src/worker --logHeapUsage --maxWorkers=1 --forceExit --env=node",
    "test:client": "REACT_APP_ENV=test react-scripts test src/client --logHeapUsage --env=jsdom",
    "test:client:ci": "REACT_APP_ENV=test CI=true react-scripts test src/client --logHeapUsage --maxWorkers=1 --env=jsdom",
    "test:server:worker:ci": "NODE_ENV=test yarn test:server:ci && yarn test:worker:ci",
    "test:once": "run-p -c test:server:worker:ci test:client:ci",
    "eject": "react-scripts eject",
    "transformOfficerCsvData": "node -e \"require('./scripts/transformOfficerCsvData')\""
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "@material-ui/core": "~3.9.2",
    "@material-ui/icons": "~3.0.0",
    "auth0": "~2.16.0",
    "auth0-js": "~9.10.0",
    "autosuggest-highlight": "~3.1.1",
    "aws-sdk": "~2.433.0",
    "axios": "~0.18.0",
    "body-parser": "~1.18.3",
    "boom": "~7.3.0",
    "busboy": "~0.3.0",
    "chromedriver": "~2.46.0",
    "connected-react-router": "~6.3.2",
    "cookie-parser": "~1.4.4",
    "csv-parse": "~4.3.4",
    "csv-stringify": "~5.3.0",
    "express": "~4.16.4",
    "express-jwt": "~5.3.1",
    "express-jwt-authz": "~2.3.0",
    "express-winston": "~3.1.0",
    "fflip": "~4.0.0",
    "fflip-express": "~1.0.2",
    "file-saver": "~2.0.1",
    "handlebars": "~4.1.2",
    "helmet": "~3.16.0",
    "history": "~4.9.0",
    "html-pdf": "~2.2.0",
    "jsonwebtoken": "~8.5.0",
    "jwks-rsa": "~1.4.0",
    "kue": "~0.11.6",
    "lodash": "~4.17.11",
    "moment": "~2.24.0",
    "moment-timezone": "~0.5.23",
    "newrelic": "~5.6.0",
    "nodemon": "~1.18.10",
    "pg": "~7.9.0",
    "pg-hstore": "~2.3.2",
    "rc-pagination": "~1.17.8",
    "react": "~16.8.4",
    "react-autosuggest": "~9.4.3",
    "react-dom": "~16.8.4",
    "react-dropzone-component": "~3.2.0",
    "react-pdf": "4.0.0-beta.6",
    "react-quill": "~1.3.3",
    "react-redux": "~6.0.1",
    "react-router": "~5.0.0",
    "react-router-dom": "~5.0.0",
    "react-scripts": "~2.1.8",
    "react-select": "^2.4.2",
    "react-text-mask": "~5.4.3",
    "redux": "~4.0.1",
    "redux-form": "~8.1.0",
    "redux-form-material-ui": "~5.0.0-beta.3",
    "redux-thunk": "~2.3.0",
    "selenium-server": "~3.141.59",
    "sequelize": "^4.0.0",
    "sequelize-cli": "~5.4.0",
    "shortid": "~2.2.14",
    "striptags": "~3.1.1",
    "timekeeper": "~2.2.0",
    "umzug": "~2.2.0",
    "validator": "~10.11.0",
    "winston": "~3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "~7.2.3",
    "@babel/core": "~7.4.0",
    "@babel/node": "~7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-syntax-json-strings": "~7.2.0",
    "@babel/preset-env": "~7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "~7.4.0",
    "concurrently": "~4.1.0",
    "enzyme": "~3.9.0",
    "enzyme-adapter-react-16": "~1.11.0",
    "enzyme-to-json": "~3.3.5",
    "jest": "~23.6.0",
    "jest-each": "~23.6.0",
    "jest-extended": "^0.11.1",
    "nightwatch": "~1.0.19",
    "nock": "~10.0.6",
    "node-mocks-http": "~1.7.3",
    "prettier": "~1.16.4",
    "promise-retry": "~1.1.1",
    "redux-devtools-extension": "~2.13.8",
    "supertest": "~4.0.0",
    "yarn-run-all": "~3.1.1"
  }
}
